
rules_version = '2';

service cloud.firestore {
  match /databases/{database}/documents {
    // WARNING: This rule is insecure and should only be used for initial testing.
    // It allows anyone to read and write to your entire database.
    // Replace this with more secure rules before launching your application.
    match /{document=**} {
      allow read, write: if true;
    }
  }
}
